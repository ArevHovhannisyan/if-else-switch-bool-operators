// if/else we use for testing the condition.When "if" condition is true
// the compilier printing the condition which have in condition "if",
// if not will printing "else" condition

#include <iostream>
using namespace std;

int main()
{
	int a, b;
	cout << "First number: ";
	cin >> a;
	cout << "Second number: ";
	cin >> b;
	
	if(a > b)
	{
	    cout <<"YES";
	}
    else
        cout <<"NO";
 
	return 0;
}

// Какое из чисел больше?
#include <iostream>
using namespace std;

int main()
{
    int n1, n2;
    cin >> n1, 
    cin >> n2;
    
    if(n1 > n2)
    {
        cout << "1" << endl;
    }
    else if(n1 < n2)
    {
        cout << "2" << endl;
    }
    else
    {
        cout << "0" << endl;
    }
    
    return 0;
}

// Максимум из трех
#include <iostream>
using namespace std;

int main()
{
    int n1, n2, n3;
    cout << "Which is the bigger number?" << endl;
    cout << "n1:";
    cin >> n1;
    cout << "n2:";
    cin >> n2;
    cout << "n3:";
    cin >> n3;
    
    if(n1 > n2)
    {
        cout << n1 << "Is the bigger number" << endl;
    }
    else if(n2 > n3)
    {
        cout  << n2 << "Is the bigger number" << endl;
    }
    else
    {
        cout << n3 << "Is the bigger number" << endl;
    }
    
    return 0;
}